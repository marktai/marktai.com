
server {
    listen 80;
    server_name minecraft.marktai.com;
	location /upload {
		auth_basic "Restricted Content";
		auth_basic_user_file /home/mtai/minecraft/.htpasswd;
		autoindex on;
		autoindex_exact_size off;
		alias /home/mtai/minecraft/upload;
		expires 1M;
		add_header Cache-Control "public";
	}
}
